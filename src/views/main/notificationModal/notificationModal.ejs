<div class="modal fade" id="notificationModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Tất Cả Thông Báo</h4>
            </div>
            <div class="modal-body">
                <div class="mark-notif-as-read">
                   <a href="javascript:void(0)" id="modal_notif_readed"><strong>Đánh Dấu Tất Cả Là Đã Đọc</strong></a>
                </div>
                <hr>
                <ul class="list-notifications">    
                    <% notifications.forEach(function(notification){ %>
                        <li>
                          <%- notification %>
                        </li>
                 <%  }) %>
                </ul>

                <div class="mark-read_more_notif">
                        <div class="load_more_spinner"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>

                        <a href="javascript:void(0)" id="modal_read_more_notif"><strong>Xem Thêm</strong></a>
                     </div>
            </div>
        </div>
    </div>
  </div>
  